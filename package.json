{
    "name": "electron-vite-template",
    "productName": "electron-app",
    "version": "0.1.0",
    "description": "Vite template for Electron application",
    "keywords": [
        "Electron",
        "React",
        "TypeScript"
    ],
    "license": "MIT",
    "author": {
        "name": "Minsup Ju",
        "email": "me@ptcookie.net",
        "url": "https://github.com/PtCookie"
    },
    "main": ".vite/build/main.js",
    "packageManager": "yarn@4.5.1",
    "type": "module",
    "scripts": {
        "postinstall": "husky",
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "eslint",
        "format": "prettier --write ."
    },
    "dependencies": {
        "@tanstack/react-router": "^1.92.0",
        "date-fns": "^4.1.0",
        "electron-log": "^5.2.0",
        "electron-squirrel-startup": "^1.0.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@electron-forge/cli": "7.4.0",
        "@electron-forge/maker-dmg": "7.4.0",
        "@electron-forge/maker-squirrel": "7.4.0",
        "@electron-forge/plugin-auto-unpack-natives": "7.4.0",
        "@electron-forge/plugin-fuses": "7.4.0",
        "@electron-forge/plugin-vite": "7.4.0",
        "@electron/fuses": "^1.8.0",
        "@eslint/compat": "^1.2.0",
        "@eslint/js": "^9.17.0",
        "@reforged/maker-appimage": "^4.0.4",
        "@tanstack/eslint-plugin-router": "^1.92.0",
        "@tanstack/router-devtools": "^1.92.0",
        "@tanstack/router-plugin": "^1.91.0",
        "@types/electron-squirrel-startup": "^1.0.2",
        "@types/node": "^20.0.0",
        "@types/react": "^19.0.0",
        "@types/react-dom": "^19.0.0",
        "electron": "33.2.0",
        "eslint": "^9.17.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-react": "^7.37.0",
        "globals": "^15.14.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.2.10",
        "prettier": "3.3.3",
        "typescript": "~5.7.0",
        "typescript-eslint": "^8.18.0",
        "vite": "^6.0.0"
    },
    "engines": {
        "node": "^20.18.0 || ^22.0.0"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint",
            "prettier --write"
        ],
        "*.{mjs,cjs}": [
            "eslint"
        ],
        "*.json": [
            "prettier --write"
        ]
    }
}
